from random import *

x = randint(1,7)

if x == 1:
    print('1000 монет')
if x == 2:
    print('Поднятие Ермака (эпическое)')
if x == 3:
    print('взлом айайойайайойай0010110110101001 (редкое)')
if x == 4:
    print('понос (калловое)')
if x == 5:
    print('кибертическая сеть (эпическое)')
if x == 6:
    print('банан (обычный)')
if x == 7:
    print('5000 монет')
