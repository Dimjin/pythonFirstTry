from random import *

x = randint(1,7)

if x == 1:
    print('1000 монет')
if x == 2:
    print('Превращение в кличку(легендарное)')
if x == 3:
    print('бессмертие на 5 секунд (мифическое)')
if x == 4:
    print('АК-47 (легендарное)')
if x == 5:
    print('кибертическая сеть (эпическое)')
if x == 6:
    print('огнемёт (имба)')
if x == 7:
    print('10000 монет')
